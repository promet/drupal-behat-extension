{
  "name": "promet/drupal-behat-extension",
  "license": "MIT",
  "description": "The commonly used Behat functionality used by Promet in Behat testing using Drupal.",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Allan Chappell",
      "email": "allan@prometsource.com"
    },
    {
      "name": "Will Milton",
      "email": "will@prometsource.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "http://static.drupal-packagist.org/v0.2.0/"
    }
  ],
  "autoload": {
    "psr-4": {
      "Promet\\Drupal\\": "src/"
    }
  },
  "require": {
    "behat/mink-zombie-driver": "*",
    "behat/mink-extension": "~1.2",
    "behat/behat": "~3.0,>=3.0.5",
    "composer/composer": "~1.0@alpha",
    "drupal/devel": "~7.1.3",
    "drupal/drupal-extension": "~3.0,>=3.0.5"
  }
}

